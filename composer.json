{
    "name": "spryker-demo/frontend-configurator-storage",
    "type": "library",
    "description": "FrontendConfiguratorStorage module",
    "license": "proprietary",
    "require": {
        "php": ">=8.2",
        "spryker-demo/frontend-configurator": "^1.0.0",
        "spryker/kernel": "^3.75.0",
        "spryker/publisher-extension": "^1.0.0",
        "spryker/storage": "^3.19.2",
        "spryker/synchronization-behavior": "^1.0.0",
        "spryker/synchronization-extension": "^1.2.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*",
        "spryker/propel-orm": "*"
    },
    "autoload": {
        "psr-4": {
            "SprykerDemo\\": "src/SprykerDemo/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerDemoTest\\": "tests/SprykerDemoTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "suggest": {
        "spryker/propel-orm": "ADD SUGGEST DESCRIPTION"
    }
}
